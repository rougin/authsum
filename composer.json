{
    "name": "rougin/authsum",
    "description": "Yet another PHP authentication library.",
    "keywords": ["authentication", "authsum", "php-library"],
    "license": "MIT",
    "authors":
    [
        {
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io/",
            "name": "Rougin Gutib",
            "role": "Software Engineer"
        }
    ],
    "require":
    {
        "php": ">=5.3.0"
    },
    "require-dev":
    {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload":
    {
        "psr-4":
        {
            "Rougin\\Authsum\\": "src/"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Rougin\\Authsum\\": "tests/"
        }
    },
    "scripts":
    {
        "test": "phpunit"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        }
    }
}